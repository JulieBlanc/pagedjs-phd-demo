{
    "style": {
        "directory": "/assets/css", 
        "files": [
            "style.css"
        ]
    },
    "customHandlers": {
        "directory": "/assets/js",
        "files": [
            "custom-handler-example-1.js",
            "custom-handler-example-2.js"
        ]
    },
    "toc":{
        "enabled": true,
        "container": "#toc-here",
        "titles":[
           ".chapter h2",
           ".chapter h3",
           ".chapter h4",
           ".chapter h5"
        ],
        "style":"leaders",
        "leaders": true,
        "counters": true,
        "beforepagenumber": ""
     },
    "notes": {
        "enabled": true,
        "type": "footnote",
        "resetCounter": ".chapter",
        "callInput": ".footnote-ref", 
        "containerNotes": "#footnotes"

    }

    
 }