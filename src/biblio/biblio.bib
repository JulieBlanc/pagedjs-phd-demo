@article{engestrom_discursive_2011,
  title = {Discursive Manifestations of Contradictions in Organizational Change Efforts: {{A}} Methodological Framework},
  shorttitle = {Discursive Manifestations of Contradictions in Organizational Change Efforts},
  author = {Engestr{\"o}m, Yrj{\"o} and Sannino, Annalisa},
  year = {2011},
