@article{rossi_ecrire_2022,
  title = {{\'Ecrire le code du code}},
  author = {Rossi, Julien},
  year = {2022},
  month = mar,
  journal = {RESET. Recherches en sciences sociales sur Internet},
  number = {11},
  publisher = {{Association Recherches en sciences sociales sur Internet}},
  issn = {2264-6221},
  doi = {10.4000/reset.3514},
  urldate = {2022-10-09},
  abstract = {Comme le laisse entendre la c\'el\`ebre formule de Lawrence Lessig selon laquelle \guillemotleft{} le code est loi \guillemotright, le code informatique qu'ex\'ecutent les ordinateurs produit des effets de r\'egulation sociale. Or, ce code est lui-m\^eme encadr\'e par tout un ensemble de standards techniques qui forment une sorte de code du code dont la r\'edaction est porteuse de nombreux enjeux techno-politiques d\'ebattus dans des ar\`enes sp\'ecialis\'ees. Cet article propose des pistes m\'ethodologiques sur la fa\c{c}on d'investiguer ces controverses qui encadrent l'\'ecriture de ces standards. Ces pistes prennent appui sur un travail d'enqu\^ete de terrain sur des projets relatifs \`a la protection de la vie priv\'ee par deux groupes de travail du World Wide Web Consortium, l'organisme qui publie les standards du Web. Les conditions particuli\`eres de fonctionnement d'un tel organisme conduit ses membres \`a communiquer essentiellement en ligne. Souvent, ces \'echanges sont archiv\'es publiquement, donnant notamment acc\`es \`a de vastes corpus d'e-mails \'echang\'es sur des listes de discussion d\'edi\'ees. La norme du consensus qui r\`egne dans ces ar\`enes, et qui d\'ecoule de la nature particuli\`ere des formes d'\'enonc\'es performatifs que sont les standards techniques, complique toutefois le travail d'interpr\'etation qui ne peut faire l'\'economie d'une d\'emarche ethnographique plus classique.},
  copyright = {All rights reserved},
  langid = {french},
  keywords = {consensus,controverses,ethnographie,IETF,politique,régulation,standards,technique,W3C},
  file = {/Users/julieblanc/Documents/RECHERCHE/biblio/Divers/programmation/rossi_ecrire_2022.pdf}
}

